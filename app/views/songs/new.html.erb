<%= form_for (@song) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :artist %>
<%= f.text_field :artist %>
<%= f.label :artist_id %>
<%= f.text_field :artist_id %>
<%= f.label :genre %>
<%= f.text_field :genre %>
<%= f.label :genre_id %>
<%= f.text_field :genre_id %>
<%= f.submit "Create Song" %>
<% end %>